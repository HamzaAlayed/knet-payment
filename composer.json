{
    "name": "developerh/knet",
    "description": "Laravel package for KNet integration",
    "type": "library",
    "require": {
        "illuminate/routing": "^5.5"
    },
    "version": "0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Hamza Alayed",
            "email": "developerh108@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "DeveloperH\\Knet\\KNetServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "DeveloperH\\Knet\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "sebastian/phpcpd": "^4.1",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "^2.6"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
